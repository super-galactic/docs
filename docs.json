{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Super Galactic",

  "appearance": {
    "default": "dark",
    "toggle": false
  },

  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },

  "favicon": "/favicon.svg",

  "logo": {
    "light": "/logo/super-galactic2-logo.png",
    "dark": "/logo/super-galactic-logo.png"
  },

  "navbar": {
    "primary": {
      "type": "button",
      "label": "Get started",
      "href": "https://app.ufogaming.io/get-started"
    }
  },

  "navigation": {
    "anchors": [
      {
        "anchor": "Main site",
        "icon": "globe",
        "href": "https://ufogaming.io"
      },
      {
        "anchor": "Twitter",
        "icon": "x",
        "href": "https://x.com/ufogaming"
      },
      {
        "anchor": "YouTube",
        "icon": "youtube",
        "href": "https://www.youtube.com/@UFOGaming"
      },
      {
        "anchor": "Telegram",
        "icon": "send",
        "href": "https://t.me/ufogaming"
      }
    ]
  },

  "icons": {
    "library": "lucide"
  },

  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },

  "footer": {
    "socials": {
      "x": "https://x.com/ufogaming",
      "youtube": "https://www.youtube.com/@UFOGaming",
      "telegram": "https://t.me/ufogaming"
    }
  }
}
