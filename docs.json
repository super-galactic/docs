{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Super Galactic",

  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },

  "favicon": "/favicon.svg",

  "logo": {
    "light": "/logo/super-galactic2-logo.png",
    "dark": "/logo/super-galactic-logo.png"
  },

  "navbar": {
    "primary": {
      "type": "button",
      "label": "Get started",
      "href": "https://app.ufogaming.io/get-started"
    }
  },

  "navigation": {
    "anchors": [
      { "anchor": "Main site", "icon": "globe", "href": "https://ufogaming.io" },
      { "anchor": "Twitter", "icon": "x", "href": "https://x.com/ufogaming" },
      { "anchor": "YouTube", "icon": "youtube", "href": "https://www.youtube.com/@UFOGaming" },
      { "anchor": "Telegram", "icon": "send", "href": "https://t.me/ufogaming" }
    ],

    "tabs": [
      {
        "tab": "Game",
        "icon": "gamepad",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "game/overview/introduction",
              "game/overview/market-opportunity",
              "game/overview/competitive-landscape",
              "game/overview/ecosystem-architecture"
            ]
          },
          {
            "group": "Gameplay",
            "pages": [
              "game/gameplay/gameplay-overview",
              "game/gameplay/progression-and-game-modes"
            ]
          },
          {
            "group": "NFTs & Assets",
            "pages": [
              "game/nfts-assets/the-originals",
              "game/nfts-assets/rarity-attributes-rating",
              "game/nfts-assets/upgrades-and-progression",
              "game/nfts-assets/equipment-and-weapons",
              "game/nfts-assets/mystery-lootboxes",
              "game/nfts-assets/breeding-system",
              "game/nfts-assets/renting-system",
              "game/nfts-assets/lore"
            ]
          },
          {
            "group": "Game Economy",
            "pages": [
              "game/economy/economy-overview",
              "game/economy/rewards-system",
              "game/economy/token-sinks",
              "game/economy/burn-mechanisms",
              "game/economy/breeding-economics"
            ]
          },
          {
            "group": "Tokenomics",
            "pages": [
              "game/tokenomics/uap-token-overview",
              "game/tokenomics/smart-contract-audit",
              "game/tokenomics/supply-and-allocation",
              "game/tokenomics/tge-details",
              "game/tokenomics/burn-tax-mechanics"
            ]
          },
          {
            "group": "Roadmap & Development",
            "pages": [
              "game/roadmap/product-roadmap",
              "game/roadmap/development-updates"
            ]
          }
        ]
      },

      {
        "tab": "Marketplace",
        "icon": "store",
        "groups": [
          {
            "group": "Marketplace",
            "pages": ["marketplace/overview"]
          }
        ]
      },

      {
        "tab": "Multichain",
        "icon": "shuffle",
        "groups": [
          {
            "group": "Multichain",
            "pages": ["multichain/overview"]
          }
        ]
      },

      {
        "tab": "Resources",
        "icon": "book-open",
        "groups": [
          {
            "group": "Resources",
            "pages": ["resources/overview"]
          }
        ]
      }
    ]
  },

  "icons": {
    "library": "lucide"
  },

  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },

  "footer": {
    "socials": {
      "x": "https://x.com/ufogaming",
      "youtube": "https://www.youtube.com/@UFOGaming",
      "telegram": "https://t.me/ufogaming"
    }
  }
}
