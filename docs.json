{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Super Galactic",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",

  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Main site",
          "href": "https://example.com",
          "icon": "globe"
        },
        {
          "anchor": "Twitter",
          "href": "https://twitter.com/yourhandle",
          "icon": "twitter"
        },
        {
          "anchor": "YouTube",
          "href": "https://youtube.com/yourchannel",
          "icon": "youtube"
        },
        {
          "anchor": "Telegram",
          "href": "https://t.me/yourchannel",
          "icon": "telegram"
        }
      ]
    },

    "tabs": [
      {
        "tab": "Game",
        "icon": "gamepad",
        "groups": [
          {
            "group": "Whitepaper",
            "pages": [
              "whitepaper/executive-summary",
              "whitepaper/background"
            ]
          }
        ]
      },
      {
        "tab": "Marketplace",
        "icon": "store",
        "groups": [
          {
            "group": "Marketplace",
            "pages": [
              "marketplace/overview",
              "marketplace/nfts",
              "marketplace/breeding",
              "marketplace/claim-uap"
            ]
          }
        ]
      },
      {
        "tab": "Multichain",
        "icon": "layer-group",
        "groups": [
          {
            "group": "Multichain",
            "pages": [
              "multichain/overview",
              "multichain/ethereum",
              "multichain/solana",
              "multichain/avalanche",
              "multichain/bridging"
            ]
          }
        ]
      }
    ]
  },

  "logo": {
    "light": "/logo/super-galactic2-logo.png",
    "dark": "/logo/super-galactic-logo.png"
  },

  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:hi@mintlify.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },

  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },

  "footer": {
    "socials": {
      "x": "https://x.com/yourhandle",
      "github": "https://github.com/your-org",
      "linkedin": "https://linkedin.com/company/your-company"
    }
  }
}
